{
    "extends": "../../tsconfig.json",
    "compilerOptions": {
        "declaration": true,
        "rootDir": "./src",
        "outDir": "./lib"
    },
    "include": [
        "./src/**/*.ts"
    ],
    "typedocOptions": {
        "entryPoint": "./src/input-autocomplete.ts",
        "exclude": "**/*.spec.ts",
        "excludeExternals": true,
        "inputFiles": "./src",
        "mode": "modules",
        "out": "../../docs/input-autocomplete",
        "readme": "none",
        "stripInternal": true
    }
}
