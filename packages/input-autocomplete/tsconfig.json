{
    "extends": "../../tsconfig.json",
    "compilerOptions": {
        "declaration": true,
        "rootDir": "./src",
        "outDir": "./lib",
        "stripInternal": true
    },
    "include": [
        "./src/**/*.ts"
    ],
    "typedocOptions": {
        "entryPoints": ["./src/input-autocomplete.ts"],
        "exclude": ["**/*.spec.ts"],
        "excludeExternals": true,
        "out": "../../docs/input-autocomplete",
        "readme": "none"
    }
}
